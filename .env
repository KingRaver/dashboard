# .env.template - Copy to .env and fill in your values

# API Keys
CLAUDE_API_KEY=your_claude_api_key_here
COINGECKO_API_KEY=your_coingecko_api_key_here  # Optional

# Twitter Credentials (if used by your bot)
TWITTER_USERNAME=your_twitter_username
TWITTER_PASSWORD=your_twitter_password

# Google Sheets Config (if used by your bot)
GOOGLE_SHEETS_PROJECT_ID=your_project_id
GOOGLE_SHEETS_PRIVATE_KEY=your_private_key
GOOGLE_SHEETS_CLIENT_EMAIL=your_client_email
GOOGLE_SHEET_ID=your_sheet_id

# Dashboard Config
DASHBOARD_SECRET_KEY=generate_a_random_secret_key_here
DASHBOARD_ADMIN_USERNAME=admin
DASHBOARD_ADMIN_PASSWORD=secure_password_here

# Browser Automation Config (if used by your bot)
CHROME_DRIVER_PATH=/usr/local/bin/chromedriver

# Database Configuration
DATABASE_PATH=data/crypto_history.db

# Optional: Analytics
ENABLE_ANALYTICS=false
GOOGLE_ANALYTICS_ID=your_analytics_id
